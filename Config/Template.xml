<?xml version="1.0" encoding="ISO-8859-1"?>
<Machine 
    MachineName="null" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="..\Schema\Powershell.xsd"> <!--$env:COMPUTERNAME for MachineName-->

    <StartScript ClearHost="true"></StartScript>
    <!-- Directory paths -->
    <Directories>
        <!-- Manadatory Directory Aliases for functions/Classes -->
        <Directory alias='PowershellRepoPath'>null</Directory>

        <!-- User Defined -->
        <Directory alias='main'>null</Directory>
    </Directories>

    <!-- Objects User uses while on the command line -->
    <Objects>
        <Object HasClass="false" VarName="null">  
        </Object>
    </Objects>

    <!-- Aliases to call Third Party exectuables, scripts, etc -->
    <Aliases>
        <Alias Name="null">null</Alias>
    </Aliases>

    <!-- User Define Functions --> 
    <Functions>
        <Function Name="null">null</Function>
    </Functions>

</Machine>