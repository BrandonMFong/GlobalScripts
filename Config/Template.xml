<?xml version="1.0" encoding="ISO-8859-1"?>
<Machine 
    MachineName="null" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="..\Schema\Powershell.xsd"> <!--$env:COMPUTERNAME for MachineName-->

    <StartScript ClearHost="true"></StartScript>
    <!-- Directory paths -->
    <Directories>
        <!-- Manadatory Directory Aliases for functions/Classes -->
        <Directory alias='PowershellRepoPath'>null</Directory>

        <!-- User Defined -->
        <Directory alias='main'>null</Directory>
    </Directories>

    <!-- Objects User uses while on the command line -->
    <Objects>
        <Object HasClass="false" VarName="">  
        </Object>

        <Object HasClass="true" VarName="">
            <Class ClassName="" HasParams="true">
                <filename></filename>
            </Class>
        </Object>
    </Objects>

    <Programs>
        <!-- Third party Functions -->
        <Program Alias="" Type="External"></Program>

        <!-- User Defined Functions/scripts --> 
        <Program Alias="" Type="Internal"></Program>
    </Programs>

    <Modules>
        <Module>\Modules\Miscellaneous.psm1</Module>
    </Modules>

</Machine>